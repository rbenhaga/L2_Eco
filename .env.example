# Example .env file for frontend
# Copy to .env.local in home-site/ directory

# Backend API URL
VITE_API_URL=http://localhost:3001

# Internal API Key (same as backend AI_INTERNAL_API_KEY)
# This allows frontend to authenticate when both are on same server
# Generate with: openssl rand -hex 32
VITE_AI_INTERNAL_API_KEY=your_random_64_char_hex_key_here

# Firebase configuration (already configured, keep existing values)
# VITE_FIREBASE_API_KEY=...
# VITE_FIREBASE_AUTH_DOMAIN=...
# etc.
